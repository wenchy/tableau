<?xml version='1.0' encoding='UTF-8'?>
<ToggleCfg KeyCol="FeatureToggle.EnvID">
    <!-- 添加新特性开关后，调用tools/gen_feature_toggle/search_all.py 生成代码 -->

    <FeatureToggle EnvID="0" desc="common: 通用环境的配置，需要包含全量的开关。common没有且对应环境也没有的开关会被默认为关闭。">
        <!-- 格式细节，匆删 -->
        <StructSupplement>
            <Toggle ID="protocol::TOGGLE_SAMPLE" OpenRate="0" WorldID="0" ZoneID="0" OpenTime="0,0" CloseTime="0,0" UserLimitType="0" UserSourceType="0" SysKey1="0" SysKey2="0" SysKey3="0" NotifyClosed="0" ErrCode="0" />
        </StructSupplement>
        <!-- 格式细节，匆删 -->

        <!--
        - ID				系统对应的toggleid
        - OpenRate			开放比例[0,10000], 0表示关闭，10000全开
        - WorldID			大区id, 0表示所有大区，支持','分隔配置多个，示例: 1,2；支持'-'配置连续大区,示例: 1-2 相当于配置1,2两个大区
        - ZoneID			小区id, 0表示所有小区，支持','分隔配置多个，示例: 1,2；支持'-'配置连续大区,示例: 1-2 相当于配置1,2两个小区
        - OpenTime			开启时间，逗号分隔日期和时间，示例：20190101, 0 表示20190101 0点开启; 20190101, 100000 表示20190101 10点0分0秒开启
        - CloseTime			关闭时间，逗号分隔日期和时间，示例：20190101, 0 表示20190101 0点关闭; 20190101, 100000 表示20190101 10点0分0秒关闭
        - UserLimitType		限制用户类型，0表示不限制，1限制账号uid，2限制账号openid，3限制角色
        - UserSourceType	限制用户名单类型，0表示白名单，1表示黑名单
        - SysKey1-3			系统自定义筛选key
        - NotifyClosed		开关关闭是否通知客户端，0不通知，其他通知
        - ErrCode			通知客户端错误码
        -->
        <Toggle name="测试" ID="protocol::TOGGLE_TEST" OpenRate="10000">111111111</Toggle>
        <Toggle name="匹配" ID="protocol::TOGGLE_MATCH" OpenRate="10000" />
        <Toggle name="事件" ID="protocol::TOGGLE_EVENT" OpenRate="10000" />
        <Toggle name="公会" ID="protocol::TOGGLE_GUILD" OpenRate="10000" ErrCode="Err_FT_Guild"/>
        <Toggle name="记忆(意志)出售" ID="protocol::TOGGLE_EQUIP_SELL" OpenRate="10000" />
        <Toggle name="记忆(意志)装配" ID="protocol::TOGGLE_EQUIP_SWITCH" OpenRate="10000" SysKey1="0" desc="SysKey1->EquipID"/>
        <Toggle name="记忆(意志)强化" ID="protocol::TOGGLE_EQUIP_UPGRADE" OpenRate="10000" />
        <Toggle name="意志进阶" ID="protocol::TOGGLE_WEAPON_STAR_UP" OpenRate="10000" SysKey1="0" desc="SysKey1->EquipID"/>
        <Toggle name="创建队伍" ID="protocol::TOGGLE_TEAM_CREATE_TEAM" OpenRate="10000" />
        <Toggle name="队伍匹配" ID="protocol::TOGGLE_TEAM_REQ_MATCH" OpenRate="10000" />
        <!-- 客户端 暂时没有主动加入队伍的功能，服务器也暂时关闭 -->
        <Toggle name="加入队伍" ID="protocol::TOGGLE_TEAM_JOIN_TEAM" OpenRate="0" />
        <Toggle name="悬赏解锁" ID="protocol::TOGGLE_BOUNTY_UNLOCK" OpenRate="0" />
