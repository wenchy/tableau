        <Toggle name="单局统计流水" ID="protocol::TOGGLE_GAMEPLAY_STATISTIC" OpenRate="10000" />

        <Toggle name="是否要校验版本降级登录" ID="protocol::TOGGLE_CHECK_CLIENT_VERSION_INCR" OpenRate="0" />
        <Toggle name="点券pc支付开放开关" ID="protocol::TOGGLE_PC_PAY" OpenRate="10000" />
        <Toggle name="Tcaplus状态统计开关" ID="protocol::TOGGLE_TCAP_CMD_STAT" OpenRate="10000" />
        <Toggle name="添加道具，背包满是否当错误" ID="protocol::TOGGLE_ITEM_BAG_FULL_IS_FAIL" OpenRate="10000" />
